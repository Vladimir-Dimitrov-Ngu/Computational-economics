   "source": [
    "class Firm:\n",
    "    '''Stores the parameters of the production function f(k) = Ak^(alpha),\n",
    "    implements the function.\n",
    "    '''\n",
    "    def __init__(self, α= 0.5, A = 2.0): # Init\n",
    "        self.α = α\n",
    "        self.A = A\n",
    "    \n",
    "    def production(self, val):\n",
    "        return self.A * val**self.α"
   ]
